    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-relay": "^5.0.0",
    "@types/use-persisted-state": "^0.3.0",
    "babel-plugin-relay": "^5.0.0",
    "echarts": "^4.2.1",
